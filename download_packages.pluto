meta_manifest:downloadFile("/B.Cache.Windows.bin", TYPE_MANIFEST)
local main_manifest = new CacheManifestReader("/B.Cache.Windows.bin")
main_manifest:downloadFile("/Packages.bin", TYPE_BIN)
